#include "calculator.h"

t_nb	*parse_expr(char *expr)
{
  t_nb	*nb;

  if (!(nb = malloc(sizeof(*nb))))
    return (0);
  return (nb);
}

int		colle2(char *expr)
{
  t_core	*core;

  if (!verif_expr(expr))
    {
      my_putstr("Syntax error\n");
      return (0);
    }
  if (!(core = malloc(sizeof(*core))))
    return (0);
  if (!(core->nb = parse_expr(expr)))
    return (0);
  return (1);
}
